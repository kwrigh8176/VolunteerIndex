{
  "name": "electron-typescript-react-mui",
  "version": "1.0.0",
  "private": true,
  "description": "Lightweight, modern boilerplate built with electron, typescript, and react.",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.ts --mode=development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack/react.webpack.ts --mode=development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.ts --mode=production",
    "build:react": "webpack --config webpack/react.webpack.ts --mode=production",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir"
  },
  "keywords": [],
  "author": "Kyle Wright",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.14.13",
    "@mui/joy": "^5.0.0-beta.11",
    "@mui/material": "^5.9.1",
    "@mui/x-charts": "^6.0.0-alpha.14",
    "@mui/x-date-pickers": "^6.15.0",
    "@reduxjs/toolkit": "^1.9.6",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "electron-router-dom": "^1.0.5",
    "got": "^11.8.5",
    "minimatch": "^3.0.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mssql": "^10.0.1",
    "nodemailer": "^6.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "semver": "^7.5.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.3",
    "@types/eslint-config-prettier": "^6.11.1",
    "@types/node": "^16.11.45",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.6",
    "@types/validator": "^13.11.2",
    "@types/wait-on": "^5.3.2",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "electron": "^22.3.25",
    "electron-builder": "^23.3.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "build": {
    "appId": "com.company.app",
    "productName": "VolunteerIndex",
    "mac": {
      "category": "public.app-category.video"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "resolutions": {
    "react": "18.2.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.13.0"
  },
  "peerDependecies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
